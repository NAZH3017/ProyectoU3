public class NodoLD{//Empleado
    private int numEmpl;
    private double impPerc;
    private double impDeduc;
    private int horasExtra;
    private double ingresoNeto;
    private NodoLista sig;
    private NodoLista ant;
//------------------------------------------------------------------------------Constructor
    public NodoLD (int numEmpl, double impPerc, double impDeduc, int horasExtra, double ingresoNeto){
        this.numEmpl=numEmpl;
        this.impPerc=impPerc;
        this.impDeduc=impDeduc;
        this.horasExtra=horasExtra;
        this.ingresoNeto=ingresoNeto;
        ant=sig=null;
    }
//------------------------------------------------------------------------------Get's y Set's
public NodoLista getSig() {
        return sig;
    }

    public void setSig(NodoLista sig) {
        this.sig = sig;
    }

    public NodoLista getAnt() {
        return ant;
    }

    public void setAnt(NodoLista ant) {
        this.ant = ant;
    }

    public int getNumEmpl() {
        return numEmpl;
    }

    public void setNumEmpl(int numEmpl) {
        this.numEmpl = numEmpl;
    }

    public double getImpPerc() {
        return impPerc;
    }

    public void setImpPerc(double impPerc) {
        this.impPerc = impPerc;
    }

    public double getImpDeduc() {
        return impDeduc;
    }

    public void setImpDeduc(double impDeduc) {
        this.impDeduc = impDeduc;
    }

    public int getHorasExtra() {
        return horasExtra;
    }

    public void setHorasExtra(int horasExtra) {
        this.horasExtra = horasExtra;
    }

    public double getIngresoNeto() {
        return ingresoNeto;
    }

    public void setIngresoNeto(double ingresoNeto) {
        this.ingresoNeto = ingresoNeto;
    }

}
